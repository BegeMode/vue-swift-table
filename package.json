{
  "name": "vue-swift-table",
  "version": "0.2.1",
  "description": "A high-performance, lightweight data table for Vue 3 with virtual scrolling, sorting, and pinning.",
  "keywords": [
    "vue",
    "vue3",
    "table",
    "datatable",
    "virtual-scroll",
    "performance",
    "typescript",
    "grid"
  ],
  "author": "BegeMode",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BegeMode/vue-swift-table.git"
  },
  "bugs": {
    "url": "https://github.com/BegeMode/vue-swift-table/issues"
  },
  "homepage": "https://github.com/BegeMode/vue-swift-table#readme",
  "type": "module",
  "main": "./dist/vue-swift-table.umd.cjs",
  "module": "./dist/vue-swift-table.es.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-swift-table.es.js",
      "require": "./dist/vue-swift-table.umd.cjs"
    },
    "./style.css": "./dist/vue-swift-table.css",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "vue-tsc -b && vite build --mode library",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "deploy": "pnpm build && gh-pages -d dist-demo"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "gh-pages": "^6.3.0",
    "prettier": "3.7.4",
    "sass": "^1.97.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  }
}
